{
  "name": "heroku-cli",
  "description": "CLI to interact with Heroku",
  "version": "6.15.22",
  "author": "Jeff Dickey @jdxcode",
  "oclif": {
    "commands": "./lib/commands",
    "scope": "heroku-cli",
    "plugins": [
      "@oclif/plugin-update",
      "@oclif/plugin-help",
      "@oclif/plugin-legacy",
      "@oclif/plugin-not-found",
      "@heroku-cli/plugin-apps",
      "@heroku-cli/plugin-run",
      "@heroku-cli/plugin-status",
      "@oclif/plugin-plugins",
      "@heroku-cli/plugin-git",
      "heroku-certs",
      "heroku-ci",
      "heroku-cli-addons",
      "heroku-cli-oauth",
      "heroku-container-registry",
      "heroku-local",
      "heroku-orgs",
      "heroku-pg",
      "heroku-pipelines",
      "heroku-ps-exec",
      "heroku-redis",
      "heroku-spaces",
      "heroku-webhooks"
    ],
    "bin": "heroku",
    "userPluginsEnabled": true,
    "dirname": "heroku",
    "npmRegistry": "https://cli-npm.heroku.com",
    "topics": {
      "2fa": {
        "description": "two-factor authentication",
        "hidden": true
      },
      "stack": {
        "description": "list available stacks",
        "hidden": true
      },
      "twofactor": {
        "description": "two-factor authentication",
        "hidden": true
      },
      "outbound-rules": {
        "description": "space outbound IP rules",
        "hidden": true
      }
    },
    "hooks": {
      "prerun": "./lib/hooks/prerun/analytics",
      "update": [
        "./lib/hooks/update/brew",
        "./lib/hooks/update/analytics",
        "./lib/hooks/update/tidy",
        "./lib/hooks/update/completions"
      ]
    },
    "s3": {
      "host": "cli-assets.heroku.com"
    }
  },
  "bin": {
    "heroku": "./bin/run"
  },
  "bugs": "https://github.com/heroku/cli/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.3",
    "@heroku-cli/command": "^8.0.0-oclif.2",
    "@heroku-cli/plugin-apps": "^2.4.27",
    "@heroku-cli/plugin-git": "^2.5.24",
    "@heroku-cli/plugin-run": "^3.5.8",
    "@heroku-cli/plugin-status": "^5.0.5",
    "@oclif/command": "^1.2.23",
    "@oclif/config": "^1.3.45",
    "@oclif/errors": "^1.0.1",
    "@oclif/plugin-help": "^1.0.1",
    "@oclif/plugin-legacy": "^1.0.1",
    "@oclif/plugin-not-found": "^1.0.1",
    "@oclif/plugin-plugins": "^1.0.0",
    "@oclif/plugin-update": "^1.0.1",
    "cli-ux": "^3.3.18",
    "debug": "3.1.0",
    "fs-extra": "5.0.0",
    "globby": "^8.0.0",
    "heroku-certs": "1.1.45",
    "heroku-ci": "1.9.5",
    "heroku-cli-addons": "1.2.26",
    "heroku-cli-oauth": "2.1.0",
    "heroku-container-registry": "4.4.6",
    "heroku-local": "5.1.21",
    "heroku-orgs": "1.7.1",
    "heroku-pg": "2.9.0",
    "heroku-pipelines": "2.5.0",
    "heroku-ps-exec": "2.3.1",
    "heroku-redis": "1.3.0",
    "heroku-spaces": "2.11.0",
    "heroku-webhooks": "1.1.0",
    "http-call": "5.0.2",
    "netrc-parser": "3.1.0",
    "semver": "5.5.0",
    "tslib": "^1.9.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.0.2",
    "@oclif/test": "^1.0.1",
    "@oclif/tslint": "^1.0.1",
    "@types/ansi-styles": "^2.0.30",
    "@types/chai": "^4.1.2",
    "@types/debug": "^0.0.30",
    "@types/execa": "^0.8.1",
    "@types/fs-extra": "5.0.0",
    "@types/mocha": "^2.2.48",
    "@types/nock": "^9.1.2",
    "@types/node": "^9.4.1",
    "@types/node-notifier": "^0.0.28",
    "@types/read-pkg": "^3.0.0",
    "@types/shelljs": "^0.7.8",
    "@types/supports-color": "3.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.17.0",
    "eslint-config-oclif": "^1.3.2",
    "mocha": "^5.0.0",
    "nock": "9.1.6",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib",
    "/yarn.lock"
  ],
  "homepage": "https://cli.heroku.com",
  "keywords": [
    "heroku"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "private": true,
  "repository": "heroku/cli",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "concurrently -p command \"tsc -p test --noEmit\" \"tslint -p test -t stylish\"",
    "postpublish": "rm .oclif.manifest.json",
    "posttest": "yarn run lint",
    "version": "oclif-dev readme --multi",
    "prepublishOnly": "yarn run build && oclif-dev manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
