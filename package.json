{
  "name": "heroku-cli",
  "description": "CLI to interact with Heroku",
  "version": "6.15.22",
  "author": "Jeff Dickey @jdxcode",
  "anycli": {
    "scope": "heroku-cli",
    "plugins": [
      "@heroku-cli/plugin-status",
      "@anycli/plugin-help",
      "@anycli/plugin-not-found"
    ],
    "bin": "heroku",
    "userPluginsEnabled": true,
    "dirname": "heroku",
    "npmRegistry": "https://cli-npm.heroku.com",
    "topics": {
      "2fa": {
        "description": "two-factor authentication",
        "hidden": true
      },
      "stack": {
        "description": "list available stacks",
        "hidden": true
      },
      "twofactor": {
        "description": "two-factor authentication",
        "hidden": true
      },
      "outbound-rules": {
        "description": "space outbound IP rules",
        "hidden": true
      }
    },
    "hooks": {
      "prerun": "./lib/hooks/prerun/analytics",
      "update": [
        "./lib/hooks/update/brew",
        "./lib/hooks/update/analytics",
        "./lib/hooks/update/tidy",
        "./lib/hooks/update/completions"
      ]
    },
    "s3": {
      "host": "cli-assets.heroku.com"
    },
    "corePlugins": [
      "heroku-apps",
      "heroku-certs",
      "heroku-ci",
      "heroku-cli-addons",
      "heroku-cli-oauth",
      "heroku-cli-status",
      "heroku-container-registry",
      "heroku-git",
      "heroku-local",
      "heroku-orgs",
      "heroku-pg",
      "heroku-pipelines",
      "heroku-ps-exec",
      "heroku-redis",
      "heroku-run",
      "heroku-spaces",
      "heroku-webhooks"
    ]
  },
  "bin": {
    "heroku": "bin/run"
  },
  "bugs": "https://github.com/heroku/cli/issues",
  "dependencies": {
    "@anycli/command": "^1.2.4",
    "@anycli/config": "^1.1.2",
    "@anycli/plugin-help": "^0.6.0",
    "@anycli/plugin-not-found": "^0.1.15",
    "@heroku-cli/color": "^1.1.3",
    "@heroku-cli/command": "^7.0.15",
    "@heroku-cli/plugin-status": "^5.0.0",
    "cli-ux": "^3.3.13",
    "debug": "3.1.0",
    "execa": "^0.9.0",
    "fs-extra": "5.0.0",
    "heroku-apps": "2.4.21",
    "heroku-certs": "1.1.44",
    "heroku-ci": "1.9.5",
    "heroku-cli-addons": "1.2.26",
    "heroku-cli-oauth": "2.1.0",
    "heroku-cli-status": "5.0.0",
    "heroku-container-registry": "4.4.5",
    "heroku-git": "2.5.23",
    "heroku-local": "5.1.21",
    "heroku-orgs": "1.7.1",
    "heroku-pg": "2.7.0",
    "heroku-pipelines": "2.5.0",
    "heroku-ps-exec": "2.3.1",
    "heroku-redis": "1.3.0",
    "heroku-run": "3.5.4",
    "heroku-spaces": "2.11.0",
    "heroku-webhooks": "1.1.0",
    "http-call": "5.0.2",
    "netrc-parser": "3.0.3",
    "semver": "5.5.0",
    "ts-lodash": "^4.0.11",
    "tslib": "^1.9.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@anycli/test": "^0.10.9",
    "@anycli/tslint": "^0.2.5",
    "@types/ansi-styles": "^2.0.30",
    "@types/chai": "^4.1.2",
    "@types/debug": "^0.0.30",
    "@types/execa": "^0.8.1",
    "@types/fs-extra": "5.0.0",
    "@types/lodash": "^4.14.100",
    "@types/mocha": "^2.2.48",
    "@types/nock": "^9.1.2",
    "@types/node": "^9.4.0",
    "@types/node-notifier": "^0.0.28",
    "@types/read-pkg": "^3.0.0",
    "@types/shelljs": "^0.7.8",
    "@types/supports-color": "3.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.17.0",
    "eslint-config-anycli": "^1.3.2",
    "mocha": "^5.0.0",
    "nock": "9.1.6",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/bin",
    "/yarn.lock"
  ],
  "homepage": "https://cli.heroku.com",
  "keywords": [
    "heroku"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "private": true,
  "repository": "heroku/cli",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "concurrently -p command \"eslint .\" \"tsc -p test --noEmit\" \"tslint -p test\"",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
